<div class="panel panel-default">
<div class="panel-heading">message list</div>
<div class="panel-body">
<% @messages.each do |message| %>
<span class="label label-info"><%= message.name %></span>
<%= message.body %>
<%= link_to "edit",edit_message_path(message) %>
<br>
<% end %>
</div>
</div>
<%# load parts%>
<%= render 'form' %>
