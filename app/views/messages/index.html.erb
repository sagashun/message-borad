<div class="panel panel-default">
  <div class="panel-heading">
    メッセージ一覧
  </div>
    
  <div class="panel-body">
    <% @messages.each do |message| %>
      <span class="label label-info"><%= message.name %></span>
       (<%= link_to "edit" , edit_message_path(message) %>)
      <%= message.body %>
      <br>
    <% end %>
  </div>
</div>
<%= render 'form' %>
